<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="IE=Edge,chrome=1">

    <title>{{ metadata.title }}</title>

    <link rel="author" href="humans.txt" />

    <meta name="description" content="{{ description if description else metadata.description }}" />
    <meta itemprop="description" content="{{ description if description else metadata.description }}">

    <meta property="og:title" content="{{ metadata.title }}{% if title %} - {{ title }}{% endif %}" />

    <meta name="format-detection" content="telephone=no">
    <meta property="og:site_name" content="{{ metadata.title }}">
    <meta property="og:title" content="{{ metadata.title }}">
    <meta property="og:url" content="https://secondofmay.co.uk">
    <meta property="og:type" content="website">
    <meta name="twitter:card" content="summary_large_image">
    <meta name="twitter:url" content="https://secondofmay.co.uk">
    <meta name="twitter:title" content="{{ metadata.title }}">
    <meta name="twitter:description" content="{{ description if description else  metadata.description }}">
    <meta property="og:description" content="{{ description if description else  metadata.description }}">
    <meta name="twitter:image" content="https://secondofmay.co.uk/assets/img/opengraph.jpg">
    <meta property="og:image" content="https://secondofmay.co.uk/assets/img/opengraph.jpg">
    <meta property="og:image:alt" content="{{ description if description else metadata.description }}">

    <link rel="shortcut icon" href="/assets/svg/jn.svg" type="image/svg" />

    {% if canonical %}
    <link rel="canonical" href="{{ canonical }}" />
    {% endif %}

    <link rel="stylesheet" media="screen" type="text/css" href="/assets/css/main.min.css" />
    <link rel="preload" as="font" type="font/woff2" href="/assets/fonts/oswald/oswald-v31-latin-regular.woff2" crossorigin>
  </head>

  <body class="no-js">
    <div class="siteContainer">

      <header class="spine">
        <hgroup class="spine__text">
          <h1>James &amp; Nat</h1>
          <h2>Second of May 2020</h2>
        </hgroup>

        <a class="spine__logo" href="/" title="Go to the homepage" aria-label="Go to the homepage">
          <svg height="91" viewBox="0 0 91 91" width="91" xmlns="http://www.w3.org/2000/svg">
            <path d="m31.111 26.166 6.707 10.185v27.04c0 5.912-.348 10.124-1.045 12.633-.697 2.51-1.888 4.671-3.571 6.485-1.552 1.708-3.327 3.009-5.326 3.902-1.9722457.8877479-4.1111712 1.3445792-6.274 1.3400341-1.735 0-3.103-.3870341-4.103-1.1630341-.999-.775-1.499-1.833-1.499-3.173 0-.946.256-1.774.77-2.483.461-.639 1.109-1.17 1.943-1.597l.287-.138 7.85 4.77c1.448-.473 2.52-1.728 3.216-3.764.666-1.944 1.014-4.685 1.044-8.22l.002-.512v-45.305zm6.542-10.264v2.024l27.662 41.63v-38.216l-1.464-1.468-4.868-.715v-3.254h17.017v3.254l-4.868.715-1.464 1.468v49.09h-5.342l-35.093-50.797-6.816-.477v-3.254zm-3.525-12.83c1.277 0 2.372.46 3.284 1.38.913.92 1.369 2.023 1.369 3.31 0 1.288-.456 2.378-1.369 3.272-.912.893-2.007 1.34-3.284 1.34s-2.371-.447-3.284-1.34c-.912-.894-1.368-1.984-1.368-3.272 0-1.313.45-2.424 1.349-3.33.899-.907 2-1.36 3.303-1.36z" fill="#fff"/>
          </svg>
        </a>
      </header>

      <div class="nav">
        <a href="/">Home</a>
        <a href="/updates">Updates</a>
        <a href="/gift">Gifts</a>
        <a href="/loop">Stay in the Loop</a>
      </div>

      <main class="centralContainer">
        {{ layoutContent | safe }}
      </main>
    </div>



    <script>
      // We have JS
      document.body.classList.remove("no-js");

      // Clear Service Worker
      if(window.navigator && navigator.serviceWorker) {
        navigator.serviceWorker.getRegistrations()
        .then(function(registrations) {
          for(let registration of registrations) {
            registration.unregister();
            console.log('Bye service worker');
          }
        });
      }

